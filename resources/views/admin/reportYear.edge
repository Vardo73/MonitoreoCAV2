<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Reporte Anual</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
     <!--chartJs-->
     <script src="https://cdn.jsdelivr.net/npm/chart.js@3.3.2/dist/chart.min.js"></script>
     <style type="text/css" media="print">
         .nover {display:none}
     </style>
     <!--AXIOS-->
     <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    @entryPointScripts('reportYear')
</head>
<body>
    <div class="container" >
        <!--ENCABEZADO-->
        <div class="row ">
           <div class="col-auto" >
               <img src=" /img/LogotipoColoresOriginales.svg" alt="0" width="210" height="140" class="img-fluid d-inline-block align-text-top">    
           </div>
           <div class="col" style="text-align:center;">
               <h2>Reporte de concentraciones</h2>
               <h4 style=" opacity: 0.5;" id="lblData">{{year}}</h4>
           </div>
       </div>
       <div class="row ">
           <div class="col-auto col-9" >
               <h3>Estación: {{station.name}}</h3>
           </div>
           <div class="col-auto col-3">
               <h5 id="IdEstacion" style="opacity: 0.5;" name='{{station.id}}'>ID Estación:{{station.slug}}</h5>
           </div>
       </div>
       <br>

       <!--GRAFICAS-->
       <h2>Grafica de pastel</h2>
       <br>
       <div class="row justify-content-md-center">
           <div class="col-auto">
               <div class="chart-container" style="position: relative; height:400px; width:400px">
                   <canvas id="piePM2" ></canvas>
               </div>
           </div>
           <div class="col-auto">
               <div class="chart-container" style="position: relative; height:400px; width:400px">
                   <canvas id="piePM10" ></canvas>
               </div>
           </div>
       </div>
       <br>
       <h2>Límites permisibles</h2>
       <br>
       <div class="row justify-content-md-center">
           <div class="col-auto">
               <div class="chart-container" style="position: relative; height:400px; width:600px">
                   <canvas id="LimitsPM2" ></canvas>
               </div>
           </div>
           <div class="col-auto">
               <div class="chart-container" style="position: relative; height:400px; width:600px">
                   <canvas id="LimitsPM10" ></canvas>
               </div>
           </div>
       </div>
       
       <button class="btn btn-primary nover" id="btnImprimir" >Imprimir</button>
    </div>


    <!--Bootstrap Bundle-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <!-- Moments -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.19.2/locale/es.js"></script>
</body>
</html>